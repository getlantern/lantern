package config

import (
    "github.com/getlantern/flashlight/client"
)

var fallbacks = map[string]*client.ChainedServerInfo{ {{range .fallbacks.default}}
  "fallback-{{.ip}}": &client.ChainedServerInfo{
    Addr:      "{{.ip}}:443",
    Cert:      "{{.cert}}",
    AuthToken: "{{.auth_token}}",
    Pipelined: true,
    Weight:    1000000,
    QOS:       10,
    trusted:   true,
  }, {{end}}
}
